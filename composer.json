{
    "name": "rickymcalister/phealthcheck",
    "description": "A HTTP health check library for PHP web applications",
    "keywords": ["healthcheck", "phealthcheck", "http", "psr-2"],
    "homepage": "https://github.com/rickymcalister/pheatlhcheck",
    "type": "library",
    "license": "GNU",
    "authors": [
        {
            "name": "Ricky McAlister"
        }
    ],
    "require": {
        "php": "^5.5.9",
        "symfony/http-foundation": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {"Phealthcheck\\": "src/Phealthcheck"}
    },
    "autoload-dev": {
        "psr-4": {"Phealthcheck\\": "tests/Phealthcheck"}
    },
    "scripts": {
        "test": [
            "phpunit",
            "phpcs"
        ]
    }
}
